{
    "name": "miladimos/laravel-filemanager",
    "description": "a cute laravel api,  web file manager",
    "homepage": "https://github.com/miladimos/laravel-filemanager",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-filemanager",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support",
        "file uploader",
        "uploader",
        "file manager"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "message.tome1331@gmail.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Miladimos\\FileManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladimos\\FileManager\\Providers\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Miladimos\\FileManager\\Facades\\FileManagerFacade"
            }
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/filesystem": "5.7.19",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "intervention/image": "^2.5"
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
