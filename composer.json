{
    "name": "miladimos/laravel-filemanager",
    "description": "a cute laravel api,  web file manager",
    "homepage": "https://github.com/miladimos/laravel-filemanager",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-filemanager",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support",
        "file uploader",
        "uploader",
        "file manager"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "message.tome1331@gmail.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Miladimos\\FileManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladimos\\FileManager\\Providers\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Miladimos\\FileManager\\Facades\\FileManagerFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^7",
        "nesbot/carbon": "^2.41",
        "morilog/jalali": "3.*",
        "intervention/image": "^2.5.1",
        "ramsey/uuid": "^4.1",
        "webpatser/laravel-uuid": "^3.0"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
